{
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/wsgi/(.*)",
      "dest": "api/wsgi.py"
    },
    {
      "src": "/asgi/(.*)",
      "dest": "api/asgi.py"
    }
  ]
}
